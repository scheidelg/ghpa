{
    "(regexClasses)": {
        "URIpath": "^(([0-9a-zA-Z\\$\\-_\\.\\+\\!\\*\\'\\(\\),\\/])|(%[2-9a-fA-F][0-9a-fA-F]))+$",
        "URIfile": "^(([0-9a-zA-Z\\$\\-_\\.\\+\\!\\*\\'\\(\\),])|(%[2-9a-fA-F][0-9a-fA-F]))+$"
    },
    
    "(keyClasses)": {
        "ghpaClass": {
            "organization": "",

            "(organization)": {
                "required": false,
                "createByDefault": false,
                "valueRegex": "^[0-9a-zA-Z]([0-9a-zA-Z]|\\-(?!\\-))*(?<!\\-)$"
            },

            "repository": "",

            "(repository)": {
                "required": false,
                "createByDefault": false,
                "valueRegex": "^[0-9a-zA-Z_.\\-]+$"
            },

            "branch": "",

            "(branch)": {
                "required": false,
                "createByDefault": false,
                "valueRegex": "^[^\\^\\[\\\\:\\?]+$"
            },

            "defaultHTMLfile": "",

            "(defaultHTMLfile)": {
                "required": false,
                "createByDefault": false,
                "valueRegex": "(regexClass:URIfile)"
            },

            "onlyGetBody": true,

            "(onlyGetBody)": {
                "required": false,
                "createByDefault": false
            }
        },

        "(ghpaClass)" : {
            "required": false,
            "createByDefault": false
        }
    },
    
    "tokensOnly": true,
    
    "(tokensOnly)": {
        "required": false,
        "createByDefault": true
    },

    "pageOptions": {
        "authOnly": false,

        "(authOnly)": {
            "required": false,
            "createByDefault": true
        },

        "SSO": true,

        "(SSO)": {
            "required": false,
            "createByDefault": true
        }
    },

    "(pageOptions)": {
        "required": false,
        "createByDefault": true
    },

    "loginFormOptions": {
        "loginFormFile": "",

        "(loginFormFile)": {
            "required": false,
            "createByDefault": false,
            "valueRegex": "(regexClass:URIpath)"
        }
    },

    "(loginFormOptions)": {
        "required": false,
        "createByDefault": true
    },

    "ghpaClasses": {
        "*ghpaClass": {},
        
        "(*ghpaClass)": {
            "keyClass": "ghpaClass",
            "keyRegex": "^[a-zA-Z]([0-9a-zA-Z]|([\\-_](?![\\-_])))*(?<![\\-_])$"
        },

        "global": {
            "defaultHTMLfile": "index.html",

            "(defaultHTMLfile)": {
                "createByDefault": true
            }
        },

        "(global)": {
            "keyClass": "ghpaClass",
            "createByDefault": true
        }
    },

    "(ghpaClasses)": {
        "required": false,
        "createByDefault": true
    }
}
